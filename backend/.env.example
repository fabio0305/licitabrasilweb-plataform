# Configurações do Servidor
NODE_ENV=development
PORT=3001
API_VERSION=v1

# Configurações do Banco de Dados PostgreSQL
DATABASE_URL="postgresql://licitabrasil:licitabrasil123@localhost:5432/licita_brasil_web?schema=public"
DB_HOST=localhost
DB_PORT=5432
DB_NAME=licita_brasil_web
DB_USER=username
DB_PASSWORD=password

# Configurações do Redis
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Configurações de Autenticação
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRES_IN=24h
JWT_REFRESH_SECRET=your-refresh-secret-key-here
JWT_REFRESH_EXPIRES_IN=7d

# Configurações de Email
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASSWORD=your-app-password
EMAIL_FROM=noreply@licitabrasilweb.com.br

# Configurações de Upload
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads
ALLOWED_FILE_TYPES=pdf,doc,docx,xls,xlsx,jpg,jpeg,png

# Configurações de Segurança
BCRYPT_ROUNDS=12
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Configurações de Logs
LOG_LEVEL=info
LOG_FILE=./logs/app.log

# URLs do Frontend
FRONTEND_URL=http://localhost:3000
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Configurações da API Externa (se necessário)
EXTERNAL_API_URL=
EXTERNAL_API_KEY=

# Configurações de Backup
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
