# Configurações do Frontend - LicitaBrasil Web Platform
# Ambiente de Produção

# URL da API Backend (local através do Nginx)
REACT_APP_API_URL=/api/v1

# URLs dos domínios
REACT_APP_FRONTEND_URL=https://licitabrasilweb.com.br
REACT_APP_API_BASE_URL=https://api.licitabrasilweb.com.br
REACT_APP_MONITORING_URL=https://monitoring.licitabrasilweb.com.br

# Configurações de ambiente
REACT_APP_ENV=production
REACT_APP_VERSION=1.0.0

# Configurações de WebSocket (se necessário)
REACT_APP_WS_URL=wss://api.licitabrasilweb.com.br

# Configurações de timeout
REACT_APP_API_TIMEOUT=30000

# Configurações de debug (desabilitado em produção)
REACT_APP_DEBUG=false

# Configurações de analytics (se necessário)
REACT_APP_GA_TRACKING_ID=
REACT_APP_SENTRY_DSN=

# Build otimizado
GENERATE_SOURCEMAP=false
